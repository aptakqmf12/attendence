{
  "name": "@wooriga/core",
  "private": true,
  "version": "0.0.0",
  "workspaces": [
    "packages/*"
  ],
  "packageManager": "yarn@3.6.1",
  "scripts": {
    "local:report": "yarn workspace @wooriga/report local",
    "dev:report": "yarn workspace @wooriga/report dev",
    "staging:report": "yarn workspace @wooriga/report staging",
    "prod:report": "yarn workspace @wooriga/report prod",
    "build-dev:report": "yarn workspace @wooriga/report build:dev",
    "build-staging:report": "yarn workspace @wooriga/report build:staging",
    "build-prod:report": "yarn workspace @wooriga/report build:prod"
  },
  "dependencies": {
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.14.3",
    "@mui/material": "^5.13.0",
    "@mui/system": "^5.15.7",
    "@mui/types": "^7.2.4",
    "@mui/x-data-grid": "^6.15.0",
    "axios": "^1.6.0",
    "dayjs": "^1.11.10",
    "lodash": "^4.17.21",
    "pretendard": "^1.3.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.14.2"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.196",
    "@types/node": "^20.11.16",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^6.3.0",
    "@typescript-eslint/parser": "^6.3.0",
    "@vitejs/plugin-react": "^4.0.0",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "latest",
    "eslint-plugin-react": "^7.33.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "lint-staged": ">=10",
    "prettier": "^3.2.5",
    "typescript": "^5.1.6",
    "vite": "^4.3.9",
    "vite-plugin-eslint": "^1.8.1"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ]
  }
}
